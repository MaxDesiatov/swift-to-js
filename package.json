{
  "name": "swift-to-js",
  "version": "0.0.1",
  "description": "JavaScript backend for Swift",
  "main": "dist/swift-to-js.js",
  "scripts": {
    "build:ts": "tsc",
    "build:ast": "pegjs --output dist/ast.js ast.pegjs",
    "build:types": "pegjs --output dist/types.js types.pegjs",
    "build": "mkdir -p dist && npm-run-all --parallel build:ts build:ast build:types",
    "test": "jest",
    "lint": "tslint --project tsconfig.json --fix"
  },
  "author": "Ryan Petrich <rpetrich@gmail.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/babel-core": "^6.25.5",
    "@types/babel-types": "^7.0.4",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.8",
    "jest": "^23.5.0",
    "npm-run-all": "^4.1.3",
    "pegjs": "^0.10.0",
    "ts-lint": "^4.5.1",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-types": "^6.26.0"
  }
}
